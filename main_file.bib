@article{HaitArtiguesLopez,
title = "The energy scheduling problem: Industrial case-study and constraint propagation techniques ",
journal = "International Journal of Production Economics ",
volume = "143",
number = "1",
pages = "13 - 23",
year = "2013",
note = "",
issn = "0925-5273",
doi = "http://dx.doi.org/10.1016/j.ijpe.2010.09.030",
url = "http://www.sciencedirect.com/science/article/pii/S0925527310003683",
author = "Christian Artigues and Pierre Lopez and Alain Haït",
keywords = "Production scheduling",
keywords = "Energy constraints",
keywords = "Constraint propagation",
keywords = "Energetic reasoning ",
abstract = "This paper deals with production scheduling involving energy constraints, typically electrical energy. We start by an industrial case-study for which we propose a two-step integer/constraint programming method. From the industrial problem we derive a generic problem, the Energy Scheduling Problem (EnSP). We propose an extension of specific resource constraint propagation techniques to efficiently prune the search space for EnSP solving. We also present a branching scheme to solve the problem via tree search. Finally, computational results are provided. "
}

